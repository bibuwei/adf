{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testff"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorageDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageDB'"
		},
		"AzureDataLakeAnalytics1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeAnalytics1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureMLStudio_apiKey": {
			"type": "secureString",
			"metadata": "Secure string for 'apiKey' of 'AzureMLStudio'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabaseCF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseCF'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"Azureadls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azureadls'"
		},
		"Hive_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'Hive'"
		},
		"Hive_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'Hive'"
		},
		"Hive_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'Hive'"
		},
		"spark_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'spark'"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "adlabwin"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "b250136c-5691-490b-8f85-31f74de7a4f4"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "0cdfcf8c-e2d3-4e6e-a1f3-b9e355d5ccd7"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "IT"
		},
		"Hive_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "0cdfcf8c-e2d3-4e6e-a1f3-b9e355d5ccd7"
		},
		"Hive_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "IT"
		},
		"Hive_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "187933c5-fac8-4495-92b1-7eb342d4c9d5"
		},
		"Hive_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"spark_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorageDB_connectionString": {
						"value": "[parameters('AzureBlobStorageDB_connectionString')]"
					},
					"AzureDataLakeAnalytics1_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeAnalytics1_servicePrincipalKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureMLStudio_apiKey": {
						"value": "[parameters('AzureMLStudio_apiKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabaseCF_connectionString": {
						"value": "[parameters('AzureSqlDatabaseCF_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"Azureadls_connectionString": {
						"value": "[parameters('Azureadls_connectionString')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_accountName')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Hive_clusterPassword": {
						"value": "[parameters('Hive_clusterPassword')]"
					},
					"Hive_clusterSshPassword": {
						"value": "[parameters('Hive_clusterSshPassword')]"
					},
					"Hive_servicePrincipalKey": {
						"value": "[parameters('Hive_servicePrincipalKey')]"
					},
					"spark_password": {
						"value": "[parameters('spark_password')]"
					},
					"Hive_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('Hive_properties_typeProperties_hostSubscriptionId')]"
					},
					"Hive_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('Hive_properties_typeProperties_clusterResourceGroup')]"
					},
					"Hive_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('Hive_properties_typeProperties_servicePrincipalId')]"
					},
					"Hive_properties_typeProperties_tenant": {
						"value": "[parameters('Hive_properties_typeProperties_tenant')]"
					},
					"spark_properties_typeProperties_userName": {
						"value": "[parameters('spark_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}